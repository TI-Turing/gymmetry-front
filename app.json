{
  "expo": {
    "name": "gymmetry-front",
    "slug": "gymmetry-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/G.png",
    "scheme": "gymmetryfront",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gymmetry.app",
      "infoPlist": {
        "GADApplicationIdentifier": "ca-app-pub-9653039234308284~9961057670"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/G.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.gymmetry.app",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/G.png",
      "name": "Gymmetry",
      "description": "Aplicación de gimnasio para móvil y web",
      "lang": "es",
      "scope": "/",
      "themeColor": "#ff6600",
      "backgroundColor": "#000000",
      "display": "standalone",
      "orientation": "portrait-primary",
      "preferRelatedApplications": false
    },
    "plugins": [
      "expo-router",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-9653039234308284~9630402104",
          "iosAppId": "ca-app-pub-9653039234308284~9961057670"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3c17fa5b-dc58-4e99-9ef8-4fb2381ed412"
      }
    },
    "owner": "jlap11"
  }
}
