{
  "expo": {
    "name": "gymmetry-front",
    "slug": "gymmetry-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gymmetryfront",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "Gymmetry",
      "description": "Aplicación de gimnasio para móvil y web",
      "lang": "es",
      "scope": "/",
      "themeColor": "#ff6600",
      "backgroundColor": "#000000",
      "display": "standalone",
      "orientation": "portrait-primary",
      "preferRelatedApplications": false
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
