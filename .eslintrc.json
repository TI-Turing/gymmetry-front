{
  "extends": [
    "expo"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",
    "no-unused-vars": "off",
    "react-hooks/exhaustive-deps": "off",
    "import/no-unresolved": "off",
    "react/no-unescaped-entities": "off",
    "@typescript-eslint/no-redeclare": "off",
    "consistent-return": "off",
    "no-undef": "off"
  },
  "ignorePatterns": [
    "node_modules/",
    ".expo/",
    "dist/"
  ]
}
