{
  "name": "gymmetry-front",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:local": "npx cross-env NODE_ENV=local EXPO_PUBLIC_ENV=local node env-loader.js && expo start",
    "start:dev": "npx cross-env NODE_ENV=development EXPO_PUBLIC_ENV=development node env-loader.js && expo start",
    "start:prod": "npx cross-env NODE_ENV=production EXPO_PUBLIC_ENV=production node env-loader.js && expo start",
    "android": "expo start --android",
    "android:local": "npx cross-env NODE_ENV=local EXPO_PUBLIC_ENV=local node env-loader.js && expo start --android",
    "android:dev": "npx cross-env NODE_ENV=development EXPO_PUBLIC_ENV=development node env-loader.js && expo start --android",
    "android:prod": "npx cross-env NODE_ENV=production EXPO_PUBLIC_ENV=production node env-loader.js && expo start --android",
    "ios": "expo start --ios",
    "ios:local": "npx cross-env NODE_ENV=local EXPO_PUBLIC_ENV=local node env-loader.js && expo start --ios",
    "ios:dev": "npx cross-env NODE_ENV=development EXPO_PUBLIC_ENV=development node env-loader.js && expo start --ios",
    "ios:prod": "npx cross-env NODE_ENV=production EXPO_PUBLIC_ENV=production node env-loader.js && expo start --ios",
    "web": "expo start --web",
    "web:local": "npx cross-env NODE_ENV=local EXPO_PUBLIC_ENV=local node env-loader.js && expo start --web",
    "web:dev": "npx cross-env NODE_ENV=development EXPO_PUBLIC_ENV=development node env-loader.js && expo start --web",
    "web:prod": "npx cross-env NODE_ENV=production EXPO_PUBLIC_ENV=production node env-loader.js && expo start --web",
    "web:build": "expo export --platform web",
    "web:serve": "expo export --platform web && npx serve dist",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watchAll --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:ci": "jest --ci --passWithNoTests --coverage",
    "env:setup": "node env-loader.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint:check && npm run format:check && npm run type-check && npm run test:ci",
    "pre-commit": "npm run lint:fix && npm run format && npm run type-check",
    "clean": "expo r -c",
    "clean:deps": "rm -rf node_modules package-lock.json && npm install",
    "reset": "npm run clean:deps && expo r -c",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && npm install",
    "models:generate": "node scripts/generate-models-from-cs.js",
    "dtos:generate": "node scripts/generate-dtos-from-cs.js",
    "codegen": "npm run models:generate && npm run dtos:generate"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": ["<rootDir>/__tests__/setup.ts"],
    "testMatch": [
      "**/__tests__/**/*.(test|spec).(js|jsx|ts|tsx)",
      "**/?(*.)(test|spec).(js|jsx|ts|tsx)"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/.expo/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@lottiefiles/dotlottie-react": "^0.6.5",
    "@mercadopago/sdk-react": "^1.0.4",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.2",
    "@react-navigation/native": "^7.1.6",
    "@types/dompurify": "^3.0.5",
    "axios": "^1.10.0",
    "dompurify": "^3.2.6",
    "expo": "^54.0.1",
    "expo-font": "~13.3.2",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-local-authentication": "~16.0.5",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "lottie-react-native": "^7.2.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.13.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "^13.13.5",
    "react-native-worklets": "^0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-function-sent": "^7.27.1",
    "@babel/plugin-proposal-json-strings": "^7.18.6",
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/plugin-proposal-throw-expressions": "^7.27.1",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/plugin-transform-json-strings": "^7.27.1",
    "@babel/plugin-transform-numeric-separator": "^7.27.1",
    "@react-native/eslint-config": "^0.80.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.9",
    "lint-staged": "^15.5.2",
    "npm-check-updates": "^16.0.0",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.0.0",
    "serve": "^14.2.4",
    "typescript": "~5.8.3"
  },
  "private": true
}
